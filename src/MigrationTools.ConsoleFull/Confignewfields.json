	"GitRepoMapping": null,
	"FieldMaps": [
		{
			"$type": "FieldValueMapConfig",
			"WorkItemTypeName": "Product Backlog Item",
			"sourceField": "System.State",
			"targetField": "System.State",
			"defaultValue": "New",
			"valueMapping": {
				"Approved": "Sprint ready",
				"Analyze": "Ready for grooming",
				"New": "New",
				"Committed": "In Sprint",
				"In Progress": "In Sprint",
				"On Hold": "New",
				"Removed": "Removed",
				"Done": "Released",
				"Ready for Test": "Finished",
				"Tested": "Finished"
			}
		},
		{
			"$type": "FieldValueMapConfig",
			"WorkItemTypeName": "Bug",
			"sourceField": "System.State",
			"targetField": "System.State",
			"defaultValue": "New",
			"valueMapping": {
				"Approved": "Approved",
				"Analyze": "New ",
				"New": "New",
				"Committed": "In Sprint",
				"In Progress": "In Sprint",
				"Done": "Released",
				"Removed": "Removed",
				"Ready for Test": "Finished",
				"Tested": "Finished"
			}
		},
		{
			"$type": "FieldValueMapConfig",
			"WorkItemTypeName": "Feature",
			"sourceField": "System.State",
			"targetField": "System.State",
			"defaultValue": "New",
			"valueMapping": {
				"New": "New",
				"In Progress": "In Progress",
				"Done": "Released",
				"Removed": "Removed"
			}
		},
		{
			"$type": "FieldValueMapConfig",
			"WorkItemTypeName": "Task",
			"sourceField": "System.State",
			"targetField": "System.State",
			"defaultValue": "To Do",
			"valueMapping": {
				"New": "To Do",
				"Test": "In Progress",
				"Review": "In Progress",
				"In Progress": "In Progress",
				"Done": "Done",
				"Removed": "Removed"
			}
		},
		{
			"$type": "FieldtoFieldMapConfig",
			"WorkItemTypeName": "Feature",
			"sourceField": "Custom.CrucialChange",
			"targetField": "Custom.SignificantChange",
			"defaultValue": null
		}
	],
	"Processors": [
		{
			"$type": "WorkItemMigrationConfig",
			"ReplayRevisions": true,
			"PrefixProjectToNodes": false,
			"UpdateCreatedDate": true,
			"UpdateCreatedBy": true,
			"BuildFieldTable": false,
			"AppendMigrationToolSignatureFooter": false,
			//"WIQLQueryBit": "AND [System.WorkItemType] NOT IN ('Code Review Response', 'Code Review Request', 'Test Case','Shared Steps','Shared Parameter','Test Suite', 'Test Plan') AND [System.AreaPath] NOT UNDER 'Multivers\\Venice' AND [System.ChangedDate] >= '2022-5-15'",
			//"WIQLQueryBit": "AND [System.WorkItemType] IN ('Shared Steps','Shared Parameter') AND [System.ID] < '68000'",
			//"WIQLQueryBit": "AND [System.WorkItemType] IN ('Test Case') AND [System.ID] = '62515'",
			//"WIQLQueryBit": "AND [System.WorkItemType] IN ('Shared Parameter','Shared Steps','Test Case') AND [System.ChangedDate] >= '2022-7-1'",
			"WIQLQueryBit": "AND [System.ChangedDate] >= '2022-07-01'",
			//"WIQLQueryBit": "AND [System.ID] in ('297326','297312','297311')",
			//"WIQLQueryBit": "AND [System.ID] IN ('285343','295453','295464','295465','295466','295520','295530','295531','295545','295594','295615')",
			"WIQLOrderBit": "[System.ID]",
			"Enabled": true,
			"LinkMigration": true,
			"AttachmentMigration": true,
			"AttachmentWorkingPath": "c:\\temp\\WorkItemAttachmentWorkingFolder\\",
			"FixHtmlAttachmentLinks": true,
			"SkipToFinalRevisedWorkItemType": true,
			"WorkItemCreateRetryLimit": 5,
			"FilterWorkItemsThatAlreadyExistInTarget": false,
			"PauseAfterEachWorkItem": false,
			"AttachmentMaxSize": 480000000,
			"CollapseRevisions": false,
			"LinkMigrationSaveEachAsAdded": true,
			"GenerateMigrationComment": false,
			"NodeStructureEnricherEnabled": false
		}
	]
}